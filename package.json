{
  "name": "nodenv-env",
  "version": "0.0.0",
  "private": true,
  "description": "View environment variables pertaining to nodejs",
  "homepage": "https://github.com/nodenv/nodenv-env#readme",
  "license": "MIT",
  "author": "Ian Heggie <ian@heggie.biz> (http://www.heggie.biz)",
  "contributors": [
    "Jason Karns <jason@karns.name> (http://jason.karns.name)"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nodenv/nodenv-env.git"
  },
  "bugs": {
    "url": "https://github.com/nodenv/nodenv-env/issues"
  },
  "directories": {
    "bin": "bin",
    "test": "test"
  },
  "scripts": {
    "pretest": "[ -d nodenv ] || git clone --depth=1 https://github.com/nodenv/nodenv",
    "test": "bats ${CI:+--tap} test",
    "publish:brew": "brew-publish $npm_package_name v$npm_package_version",
    "postversion": "git push --follow-tags && npm run publish:brew"
  },
  "devDependencies": {
    "bats": "^0.4.2",
    "brew-publish": "^2.0.0"
  }
}
